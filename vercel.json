{
    "version": 2,
    "builds": [
      {
        "src": "index",
        "use": "@vercel/static"
      },
      {
        "src": "auth",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/index"
      },
      {
        "src": "/auth",
        "dest": "/auth"
      }
    ]
  }
  