{
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/auth", "destination": "/auth.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "redirects": [
    { "source": "/index.html", "destination": "/home", "permanent": true },
    { "source": "/auth.html", "destination": "/auth", "permanent": true }
  ]
}
